{
  "name": "golden-layout",
  "version": "1.5.9.2",
  "author": "deepstreamHub GmbH",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "grunt": "^0.4.5",
    "grunt-contrib-less": "^1.4.1",
    "grunt-contrib-watch": "0.5.3",
    "grunt-gulp": "^1.0.1",
    "grunt-karma": "^2.0.0",
    "grunt-release": "0.13.*",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-insert": "^0.5.0",
    "gulp-uglify": "^1.5.3",
    "handlebars": "2.0.0-alpha.2",
    "jshint": "^2.11.1",
    "karma": "0.13.*",
    "karma-chrome-launcher": "~0.1.4",
    "karma-cli": "^2.0.0",
    "karma-coverage": "0.2.4",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "~0.1.5",
    "karma-jasmine": "~0.1.5",
    "karma-phantomjs-launcher": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "tslint": "^3.7.3",
    "tslint-config-typings": "^0.2.2",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "walker": "1.0.6"
  },
  "description": "A multi-screen javascript Layout manager \r https://golden-layout.com",
  "main": "./dist/goldenlayout.js",
  "typings": "./index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "jquery": "*"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clean build-steps",
    "build-dev": "cross-env NODE_ENV=development run-s clean build-steps",
    "build-steps": "run-p build:*",
    "sass": "sass ./src/scss:./src/css -s compressed",
    "build:gulp": "gulp build",
    "build:sass": "npm run sass",
    "watch": "run-p watch:*",
    "watch:gulp": "gulp dev",
    "watch:sass": "npm run sass -- --watch --update",
    "clean": "rimraf ./dist ./src/css",
    "start": "cross-env NODE_ENV=development npm run watch",
    "test": "karma start karma.conf.js",
    "test:ci": "karma start karma.conf.js --single-run",
    "tslint": "tslint \"**/*.ts\" -e \"node_modules/**\" -e \"typings/**\"",
    "prepublish": "typings install",
    "publish": "npm -s run tslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepstreamIO/golden-layout.git"
  },
  "keywords": [
    "layout manager",
    "javascript",
    "docker",
    "layout",
    "popouts"
  ],
  "bugs": {
    "url": "https://github.com/deepstreamIO/golden-layout/issues"
  },
  "homepage": "https://github.com/deepstreamIO/golden-layout",
  "npmName": "golden-layout",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "goldenlayout.js",
        "goldenlayout.min.js"
      ]
    },
    {
      "basePath": "/src/css/",
      "files": [
        "goldenlayout.base.css",
        "goldenlayout-dark-theme.css",
        "goldenlayout-light-theme.css"
      ]
    },
    {
      "basePath": "/",
      "files": [
        "typings.json"
      ]
    }
  ],
  "jspm": {
    "main": "dist/goldenlayout",
    "format": "global",
    "registry": "jspm",
    "dependencies": {
      "jquery": "^2.1.0"
    },
    "shim": {
      "dist/goldenlayout": [
        "jquery"
      ]
    }
  }
}
